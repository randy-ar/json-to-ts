{
  "name": "@randy-ar/json-to-ts",
  "version": "1.0.0",
  "description": "Interactive CLI tool to convert JSON to TypeScript types and dummy data",
  "main": "dist/index.js",
  "bin": {
    "json-to-ts": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "prepublish": "npm run build"
  },
  "keywords": [
    "json",
    "typescript",
    "cli",
    "converter",
    "types",
    "dummy-data",
    "interactive"
  ],
  "author": "Randy AR",
  "license": "MIT",
  "dependencies": {
    "inquirer": "^9.2.12",
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
